import java.io.*;
import org.codehaus.plexus.util.*;

boolean result = true;

try
{
	File buildLOgFile = new File(basedir, "build.log").getCanonicalFile();  
    String buildLog = FileUtils.fileRead(buildLOgFile, "UTF-8" );
    
    String[] messages = {
		"[WARNING] Found duplicate (but equal) classes in [junit:junit:4.8.2,junit:junit-dep:4.8.2]",
		"[WARNING] Found duplicate (but equal) classes in [junit:junit:4.8.2,org.hamcrest:hamcrest-core:1.1]"		
	};
	
	for(String message : messages)
	{
    	if (buildLog.indexOf(message) == -1)
    	{
    		System.out.println("Did not found message '" + message + "'");
    		result = false;
    	}
    }
}
catch( Throwable t )
{
    t.printStackTrace();
    result = false;
}

return result;